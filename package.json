{
  "name": "get-ci-env",
  "version": "1.3.0",
  "description": "Get and normalize continuous integration service environment variables.",
  "type": "module",
  "main": "./src/main.cjs",
  "exports": {
    "require": "./src/main.cjs",
    "default": "./src/main.mjs"
  },
  "files": [
    "src/main.cjs",
    "src/main.mjs"
  ],
  "scripts": {
    "build": "rollup src/main.mjs --format cjs --file src/main.cjs",
    "test": "node --experimental-modules ./src/test.mjs",
    "test:ci": "npm test && npm run format -- --list-different",
    "format": "prettier --write --single-quote --trailing-comma all src/*.mjs",
    "preversion": "npm test",
    "prepack": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "tape": "^4.13.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Siilwyn/get-ci-env.git"
  },
  "author": "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)",
  "license": "GPL-3.0+",
  "keywords": [
    "ci",
    "env",
    "continuous integration",
    "environment variables",
    "buildkite",
    "circleci",
    "codefresh",
    "gitlab ci",
    "travis ci"
  ]
}
